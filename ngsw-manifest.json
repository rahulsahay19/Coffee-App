{
    "routing":{
        "index":"/index.html",
        "routes":{
            "/":{
                "match": "exact"
            },
            "/coffee":{
                "match": "prefix"
            }
    
        }
    },
    "static.ignore":[
        "^\/icons\/.*$"
    ],
    "external":{
        "urls":[
            {
                "url" : "https://fonts.googleapis.com/icon?family=Material+Icons"
            },
            {
                "url" : "https://fonts.gstatic.com/s/materialicons/v34/2fcrYFNaTjcS6g4U3t-Y5ZjZjT5FdEJ140U2DJYC3mY.woff2"
            }
        ]
    },
    "dynamic":{
        "group":[
            {
                "name" : "api",
                "urls" : {
                    "http://localhost:3000/coffees" :{
                        "match" : "prefix"
                    }
                },
                "cache" :{
                    "optimizeFor" : "freshness",
                    "networkTimeoutMs" : 1000,
                    "maxEntries" : 30,
                    "strategy" : "lru",
                    "maxAgeMs" : 36000000
                }
            }
        ]
    },
    "push" : {
        "showNotification" : true,
        "backgroundOnly" : true
    }
}